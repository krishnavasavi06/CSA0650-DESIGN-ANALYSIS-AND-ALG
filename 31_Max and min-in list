#include <stdio.h>
#include <limits.h>

#define MAX_SIZE 100

int main() {
    int n, i;
    int arr[MAX_SIZE];
    int min, max;
    printf("Krishna vasavi-192210271\n");

    printf("Enter the number of elements (max %d): ", MAX_SIZE);
    scanf("%d", &n);

    printf("Enter %d integers:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    min = INT_MAX;
    max = INT_MIN;

    for (i = 0; i < n; i++) {
        if (arr[i] < min) {
            min = arr[i];
        }
        if (arr[i] > max) {
            max = arr[i];
        }
    }

    printf("Minimum value in the list: %d\n", min);
    printf("Maximum value in the list: %d\n", max);

    return 0;
}
